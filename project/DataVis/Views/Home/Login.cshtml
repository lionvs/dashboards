<div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
        <div class="form-group">
            <input class="form-control" id="login" type="text" placeholder="Login" />
        </div>
        <div class="form-group">
            <input class="form-control" id="password" type="password" placeholder="Password" />
        </div>
        <button onclick="proxy.login($('#login').val(), $('#password').val())" class="btn btn-default">Log In</button>
        <a class="btn btn-default" href="/home/register">Register</a>
    </div>
</div>